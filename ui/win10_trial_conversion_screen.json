{
	"namespace": "win10_trial_conversion",
	//Please do not remove or edit any credit string :)
	"win10_trial_conversion_screen@common.base_screen": {
		"button_mappings": [
			{
				"from_button_id": "button.menu_cancel",
				"to_button_id": "button.menu_exit",
				"mapping_type": "global"
			}
		],
		"$screen_content": "win10_trial_conversion.win10_trial_conversion_panel",
		"cache_screen": true,
			"$screen_animations": [
			"@common.screen_exit_animation_push_fade",
			"@common.screen_exit_animation_pop_fade",
			"@common.screen_entrance_animation_push_fade",
			"@common.screen_entrance_animation_pop_fade"
		]
	},
	"win10_trial_conversion_panel@common.fullscreen_header": {
		"$back_button_has_title": true,
		"$button_text": "Exit",
		"$child_control": "win10_trial_conversion.Content"
	},
	"Content": {
		"type": "panel",
		"controls": [
		{"tab@tabbed_upsell.main_panel":{}}
		]
	}
	  
				
}