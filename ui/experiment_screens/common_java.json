  "highlight_text_label": {
    "type": "label",
    "offset": [ 5, 3 ],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "layer": 155,
    "color": "$hover_panel_text_color",
    "$hover_panel_text_color|default": [ 1.0, 1.0, 1.0 ],
    "text": "$hover_panel_text",
    "shadow": true,
    "follows_cursor": true,
    "size": "$hover_text_size",
    "$hover_text_size|default": [ "default", "default" ],
    "$hover_panel_text|default": "",
    "bindings": [
      {
        "binding_name": "$hover_panel_text"
      }
    ]
  },

  "highlight_image": {
    "type": "image",
    "size": [ "100%c+10px", "100%c+6px" ],
    "texture": "textures/container/purpleBorder",
    "alpha": 0.9375,
    "layer": 700,
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "offset": [ 14.5, -6.5 ],
    "$hover_text_label_control|default": "java_button.item_hover_text_label",
    "$hover_text_texture_bindings|default": [],
    "bindings": "$hover_text_texture_bindings",
    "controls": [
      { "text@$hover_text_label_control": { } }
    ]
  },

  "item_hover_text_label@java_button.highlight_text_label": {
    "$hover_text_binding_name|default": "#hover_text",
    "text": "$hover_text_binding_name",
    "bindings": [
      {
        "binding_name": "$hover_text_binding_name",
        "binding_type": "collection",
        "binding_collection_name": "$item_collection_name"
      }
    ]
  },

  "highlight_text_panel5": {
    "type": "panel",
    "allow_clipping": false,
    "controls": [
